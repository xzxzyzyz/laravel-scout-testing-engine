{
  "name": "xzxzyzyz/laravel-scout-testing-engine",
  "description": "Testing Engine For Laravel Scout",
  "keywords": [
    "laravel",
    "scout",
    "test",
    "engine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Xzxzyzyz",
      "email": "xzxzyzyz@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/scout": "*",
    "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Xzxzyzyz\\Laravel\\ScoutTestingEngine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Xzxzyzyz\\Laravel\\ScoutTestingEngine\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:coverage": "phpdbg -qrr vendor/bin/phpunit --coverage-html=coverage"
  }
}
